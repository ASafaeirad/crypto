{
  "name": "@frontendmonster/crypto",
  "version": "3.0.3",
  "license": "MIT",
  "author": "frontendmonster <frontendmonster@gmail.com>",
  "private": true,
  "description": "useful encryption and jwt utils",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frontendmonster/crypto.git"
  },
  "main": "./cjs/index.js",
  "module": "./esm5/index.js",
  "es2015": "./esm2015/index.js",
  "typings": "./types/index.d.ts",
  "sideEffects": false,
  "files": [
    ".npmrc",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "commit": "git-cz",
    "clean": "shx rm -rf lib/*",
    "prebuild": "npm run clean",
    "build": "tsc && tsc --project ./tsconfig.es2015.json && tsc --project ./tsconfig.cjs.json",
    "lint": "eslint ./src --ext .ts --fix",
    "release": "builder --publish",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "postinstall": "husky install"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "jsonwebtoken": "8.5.1",
    "nanoid": "3.1.18"
  },
  "devDependencies": {
    "@frontendmonster/builder": "2.1.2",
    "@fullstacksjs/eslint-config": "4.1.9",
    "@types/bcryptjs": "2.4.2",
    "@types/jsonwebtoken": "8.5.0",
    "@types/node": "14.14.10",
    "commitizen": "4.2.2",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.14.0",
    "husky": "7.0.1",
    "lint-staged": "10.5.2",
    "np": "7.0.0",
    "pinst": "2.1.6",
    "prettier": "2.2.1",
    "shx": "0.3.3",
    "typescript": "4.1.2"
  }
}
